<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile_full_bg"
    tools:context=".bookingmodule.SuccessBookingRequestActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="279dp"
        android:layout_centerInParent="true"
        android:id="@+id/imgSuccess"
        android:src="@drawable/archive_successful"/>

    <Button
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:text="My Cars"
        android:layout_below="@id/imgSuccess"
        android:layout_centerHorizontal="true"
        android:textColor="#FFF"
        android:background="@drawable/login_button"
        android:id="@+id/btnMyBookings"/>

</RelativeLayout>